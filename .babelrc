{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "7.6"
        }
      }
    ],
    "flow"
  ],
  "plugins": ["transform-class-properties", "transform-object-rest-spread"],
  "env": {
    "node6": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "6"
            }
          }
        ],
      ],
      "plugins": [
        ["transform-runtime", {
          "polyfill": false
        }]
      ]
    }
  }
}
